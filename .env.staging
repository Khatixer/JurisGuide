# JurisGuide Platform Staging Environment Configuration

# Application Environment
NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://staging.jurisguide.com

# Supabase Configuration (Staging)
NEXT_PUBLIC_SUPABASE_URL=your_staging_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_staging_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_staging_supabase_service_role_key

# Google Gemini API Configuration
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key

# Security Configuration
NEXTAUTH_SECRET=your_staging_nextauth_secret_key_minimum_32_characters
NEXTAUTH_URL=https://staging.jurisguide.com

# Monitoring and Error Tracking
NEXT_PUBLIC_SENTRY_DSN=your_staging_sentry_dsn
SENTRY_AUTH_TOKEN=your_sentry_auth_token
SENTRY_ENVIRONMENT=staging

# Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_staging_google_analytics_id

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Logging
LOG_LEVEL=info

# Performance Monitoring
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true